{
  "name": "jeyroik/extas-workflow",
  "description": "Extas workflow package",
  "require": {
    "jeyroik/extas-base": "2.*",
    "jeyroik/extas-templates": "1.*",
    "jeyroik/extas-installer": "2.*",
    "jeyroik/extas-parameters": "0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "vlucas/phpdotenv": "^3.4",
    "jeyroik/extas-repositories-mongo": "1.*",
    "jeyroik/extas-workflow-dispatchers": "0.*"
  },
  "autoload": {
    "psr-4": {
      "extas\\": "src/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox --coverage-clover=coverage.xml --whitelist=src tests"
  }
}